{
    "name": "shopware/ucp-integration",
    "description": "Universal Commerce Protocol (UCP) integration for Shopware 6",
    "type": "shopware-platform-plugin",
    "version": "1.0.0",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "UCP Contributors"
        }
    ],
    "require": {
        "php": ">=8.1",
        "shopware/core": "~6.5.0|~6.6.0|~6.7.0",
        "shopware/storefront": "~6.5.0|~6.6.0|~6.7.0",
        "symfony/http-foundation": "^5.4|^6.0|^7.0",
        "symfony/serializer": "^5.4|^6.0|^7.0",
        "justinrainbow/json-schema": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5|^10.0",
        "shopware/core": "~6.5.0|~6.6.0"
    },
    "autoload": {
        "psr-4": {
            "SwagUcp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SwagUcp\\Tests\\": "tests/"
        }
    },
    "extra": {
        "shopware-plugin-class": "SwagUcp\\SwagUcp",
        "label": {
            "de-DE": "UCP Integration",
            "en-GB": "UCP Integration"
        },
        "description": {
            "de-DE": "Universal Commerce Protocol Integration f√ºr Shopware",
            "en-GB": "Universal Commerce Protocol Integration for Shopware"
        }
    }
}
